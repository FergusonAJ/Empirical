default:
	$(MAKE) -C control/
	$(MAKE) -C evo/
	$(MAKE) -C meta/
	$(MAKE) -C tools/
	$(MAKE) -C web/

clean:
	$(MAKE) clean -C control/
	$(MAKE) clean -C evo/
	$(MAKE) clean -C meta/
	$(MAKE) clean -C tools/
	$(MAKE) clean -C web/

debug:
	$(MAKE) debug -C control/
	$(MAKE) debug -C evo/
	$(MAKE) debug -C meta/
	$(MAKE) debug -C tools/
	$(MAKE) debug -C web/
